@// $Id: lenght-list.cpp,v 1.1 2021-05-01 12:51:44-07 - - $

#include <list>
using namespace std;

size_t sum (const list<int>& lst) {
   size_t sum = 0;
   for (const auto& num: lst) sum += num;
   return sum;
}
@
