@// $Id: sum-list.cpp,v 1.1 2021-05-01 12:53:11-07 - - $

#include <vector>
using namespace std;

size_t sum (const vector<int>& lst) {
   size_t sum = 0;
   for (const auto& num: lst) sum += num;
   return sum;
}
@
